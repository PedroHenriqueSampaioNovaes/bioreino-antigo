{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "arrow-body-style": 0,
    "no-cond-assign": 0,
    "no-return-assign": 0
  }
}
